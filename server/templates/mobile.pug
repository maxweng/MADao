    link(href="{% static 'mobile//static/mobile/app.0d51359ed9c525ddc4183e1be4d953ae.css' %}", rel='stylesheet')
    // endinject
    // injectCordova
  |     
  body(ng-app='madaoClient')
    .view-page
      ion-nav-view
    |         
    script.
      window.HOST = "";
      window.STATIC_HOST = "/static/mobile/";
    // inject:js
    script(src='http://res.wx.qq.com/open/js/jweixin-1.0.0.js')
    |         
    script(src="{% static 'wechat/wechat_oauth_sdk.js' %}?appkey={{WECHAT_APP_ID}}&debug=false&_=0.1")
    |         
    script(src="{% static 'mobile//static/mobile/app.8ec6d7f850663fa7da5e740430be9f9a.js' %}")
    // endinject
    script.
      /* inject:script */
      wx.config({
      debug: false,
      appId: '{{wechat_appid}}',
      timestamp: {{timestamp}},
      nonceStr: '{{noncestr}}',
      signature: '{{wechat_signature}}',
      jsApiList: ['onMenuShareTimeline', 'onMenuShareAppMessage', 'onMenuShareQQ', 'onMenuShareWeibo', 'startRecord', 'onVoiceRecordEnd', 'stopRecord', 'onVoicePlayEnd', 'uploadVoice', 'playVoice', 'downloadVoice', 'stopVoice']
      });
